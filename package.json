{
    "author": "FabienCont",
    "description": "game built with theatre (javascript es6 game framework)",
    "devDependencies": {
        "@babel/core": "7.4.0",
        "@babel/preset-env": "7.4.2",
        "babel-loader": "8.0.5",
        "url-loader": "1.0.1",
        "webpack": "4.18.0",
        "webpack-cli": "3.1.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "4.1.4",
        "webpack-node-externals": "^1.7.2",
        "webpack-notifier": "1.6.0"
